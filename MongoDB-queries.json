db.reviews.aggregate([
    {
        $unwind: "$Reviews"
    }, 
    {
        $group: {
            _id: { author: "$Reviews.Author" },
            NumberOfReviews: {$sum: 1}           
        }
    },
    {
        $sort: {
            NumberOfReviews: -1
        }
    }
], {allowDiskUse: true}
